{"ast":null,"code":"export default {\n  data() {\n    return {\n      data: [],\n      IdUser: 0,\n      hidden: 0\n    };\n  },\n  mounted() {\n    this.getdata();\n  },\n  methods: {\n    getInfo(id) {\n      this.IdUser = id;\n      this.hidden = 1;\n    },\n    hideInfo() {\n      this.IdUser = 0;\n    },\n    async getdata() {\n      const res = await fetch(\"https://jsonplaceholder.typicode.com/users\");\n      const json = await res.json();\n      this.data = json;\n      this.nameUser = this.data;\n      console.log(this.data);\n    }\n  }\n};","map":{"version":3,"mappings":"AA0BA;EACAA;IACA;MACAA;MACAC;MACAC;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA;MACA;IACA;IACAC;MACA;IACA;IACA;MACA;MACA;MACA;MACA;MACAC;IACA;EACA;AACA","names":["data","IdUser","hidden","mounted","methods","getInfo","hideInfo","console"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <nav>\n      <router-link to=\"/\">Home</router-link> |\n      <router-link to=\"/SearchUser\">About</router-link>\n    </nav>\n    <div class=\"user\" v-for=\"users of data\" :key=\"users.id\">\n      <div class=\"block\">\n        <p class=\"oneuser\">{{ users.name }}</p>\n        <button class=\"info\" @click=\"getInfo(users.id)\">+</button>\n      </div>\n      <div v-if=\"IdUser == users.id\">\n        <div v-if=\"hidden\">\n          <button class=\"hideInfo\" @click=\"hideInfo\">-</button>\n        </div>\n        <p class=\"oneuser\">{{ users.username }}</p>\n        <p class=\"oneuser\">{{ users.email }}</p>\n        <p class=\"oneuser\">{{ users.phone }}</p>\n      </div>\n    </div>\n\n    <router-view />\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      data: [],\n      IdUser: 0,\n      hidden: 0,\n    };\n  },\n  mounted() {\n    this.getdata();\n  },\n  methods: {\n    getInfo(id) {\n      this.IdUser = id;\n      this.hidden = 1;\n    },\n    hideInfo() {\n      this.IdUser = 0;\n    },\n    async getdata() {\n      const res = await fetch(\"https://jsonplaceholder.typicode.com/users\");\n      const json = await res.json();\n      this.data = json;\n      this.nameUser = this.data;\n      console.log(this.data);\n    },\n  },\n};\n</script>\n<style>\n.user {\n  display: block;\n  border: 1px solid;\n  margin-bottom: 10px;\n  border-radius: 15px;\n  width: 300px;\n}\n.info {\n  height: 20px;\n  border-radius: 50%;\n  margin-right: 10px;\n}\n.oneuser {\n  margin-left: 10px;\n}\n.block {\n  display: flex;\n  align-items: center;\n\n  justify-content: space-between;\n}\n.hideInfo {\n  height: 20px;\n  border-radius: 50%;\n  margin-right: 10px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}